    "python.analysis.inlayHints.callArgumentNames": "all",
    "python.analysis.typeCheckingMode": "standard",
    "json.format.enable": true,
    "workbench.settings.useSplitJSON": true,
    "[python]": {
        "editor.defaultFormatter": null
    }
}